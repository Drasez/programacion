#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

int main ()
{
int suma,i,N;
cout <<"ingrese el valor de N";
cin >> N;
suma=0;
for (i=1;i<N;i=i+2)
	suma=suma+i;
	cout<<"la suma de los" <<N<< "primeros numeros naturles impares es: "<< suma;
getch ();
return 0;
	
}

